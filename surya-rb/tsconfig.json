{
	"extends": "@surya/typescript-config/esm.json",
	"compilerOptions": {
		"rootDir": "src",
		"outDir": "dist",
		"baseUrl": ".",
		"paths": {
			"@libs/*": ["src/libs/*"],
			"@handler/*": ["src/handler/*"]
		},
		"noUncheckedIndexedAccess": true
	},
	"include": ["src/**/*"],
	"exclude": ["node_modules"]
}
