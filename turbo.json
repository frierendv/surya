{
	"$schema": "https://turborepo.com/schema.json",
	"globalDependencies": [".env"],
	"globalEnv": ["NODE_ENV", "SR_*", "LOG_LEVEL"],
	"ui": "tui",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"inputs": ["$TURBO_DEFAULT$", ".env.development", ".env"],
			"outputs": [".next/**", "!.next/cache/**", "dist/**"]
		},
		"@surya/baileys-utils#build": {
			"dependsOn": ["^@surya/core#build"]
		},
		"typecheck": {
			"dependsOn": ["^typecheck"]
		},
		"clean": {
			"outputs": [],
			"cache": false
		}
	}
}
