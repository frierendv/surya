{
	"$schema": "https://turborepo.com/schema.json",
	"globalDependencies": [".env"],
	"globalEnv": [],
	"ui": "tui",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"inputs": ["$TURBO_DEFAULT$", ".env.development", ".env"],
			"outputs": [".next/**", "!.next/cache/**", "dist/**"]
		},
		"lint": {
			"dependsOn": ["^lint"]
		},
		"lint:fix": {
			"dependsOn": ["^lint:fix"]
		},
		"typecheck": {
			"dependsOn": ["^typecheck"]
		},
		"dev": {
			"cache": false,
			"persistent": true
		},
		"clean": {
			"outputs": [],
			"cache": false
		}
	}
}
